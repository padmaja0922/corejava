/**
 *  This class takes int parameter and changes it to Integer Wrapper class
 */
package com.bcj.corejava.classes.medium.lab6;

/**
 * @author Boot Camp User 008
 *
 */
public class IntegerWrapper {

	private int a;
	private Integer i;
	/**
	 * @return the i
	 */
	public Integer getI() {
		return i;
	}

	/**
	 * @param i the i to set
	 */
	public void setI(Integer i) {
		this.i = i;
	}

	/**
	 * @return the a
	 */
	public int getA() {
		return a;
	}

	/**
	 * @param a the a to set
	 */
	public void setA(int a) {
		this.a = a;
	}
	
	
  public IntegerWrapper(Integer i, int a)
	{ this.i = i;
	  this.a=a;
	}
	
	

}
