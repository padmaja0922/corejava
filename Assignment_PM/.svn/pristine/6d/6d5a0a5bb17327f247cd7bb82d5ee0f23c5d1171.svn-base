/**
 *  This class creates object of IntegerWrapper class to swap Integer to int variables
 */
package com.bcj.corejava.classes.medium.lab6;

/**
 * @author Boot Camp User 008
 *
 */
public class Test {
	
	IntegerWrapper intwap = new IntegerWrapper(34,12); 
	
   // method to swap Integer to int	
	public static void swap() {
		
		Test t = new Test();
		int a =t.intwap.getA();
		t.intwap.setA(t.intwap.getI());  // converting Integer to int
		t.intwap.setI(Integer.valueOf(a));  //converting int to integer
			
		System.out.println("value of i "+t.intwap.getI());
		System.out.println("value of a "+t.intwap.getA());
	  
		}
	 
   public static void main(String[] args) {	  
	    
	       swap ();  			
			
		}

}
